<script>
document.addEventListener('DOMContentLoaded', function() {
    const searchInput = document.getElementById('categorySearch');
    
    searchInput.addEventListener('input', debounce(function() {
        const searchTerm = this.value.toLowerCase().trim();
        
        // Search in categories section
        const categories = document.querySelectorAll('.categories-container .category-item');
        categories.forEach(category => {
            const categoryText = category.textContent.toLowerCase();
            const isVisible = categoryText.includes(searchTerm);
            category.style.display = isVisible ? '' : 'none';
        });
        
        // Search in accordion sections
        const accordionSections = document.querySelectorAll('.mb-8');
        accordionSections.forEach(section => {
            const sectionTitle = section.querySelector('h3')?.textContent.toLowerCase() || '';
            const questions = section.querySelectorAll('.accordion-item');
            let sectionHasMatch = false;
            
            questions.forEach(item => {
                const questionText = item.querySelector('.btn-link')?.textContent.toLowerCase() || '';
                const answerText = item.querySelector('.accordion-body')?.textContent.toLowerCase() || '';
                const contentText = `${questionText} ${answerText}`;
                
                const isMatch = contentText.includes(searchTerm);
                item.style.display = isMatch ? '' : 'none';
                if (isMatch) sectionHasMatch = true;
                
                // Expand matching items
                if (isMatch && searchTerm.length > 0) {
                    const collapseElement = item.querySelector('.collapse');
                    if (collapseElement && !collapseElement.classList.contains('show')) {
                        new bootstrap.Collapse(collapseElement, { show: true });
                    }
                }
            });
            
            // Show/hide entire section based on matches
            section.style.display = sectionHasMatch || sectionTitle.includes(searchTerm) ? '' : 'none';
        });
    }, 300));
    
    // Debounce function
    function debounce(func, wait) {
        let timeout;
        return function(...args) {
            clearTimeout(timeout);
            timeout = setTimeout(() => func.apply(this, args), wait);
        };
    }
});
</script>

<script>
	document.querySelectorAll('.answer p').forEach(paragraph => {
    // Collect all text nodes and their word content
    const textNodes = Array.from(
        document.createTreeWalker(paragraph, NodeFilter.SHOW_TEXT, null)
    ).map(textNode => ({
        node: textNode,
        words: textNode.nodeValue.match(/(\S+[\s\u00A0]*)/g) || [],
        originalParent: textNode.parentNode
    })); 

    // Calculate global word count and target
    const allWords = textNodes.flatMap(tn => tn.words);
    const targetCount = Math.ceil(allWords.length * 0.3);
    let wrappedWords = 0; 

    // Process each text node with DOM-safe replacement
    textNodes.forEach(({ node, words, originalParent }) => {
        const fragment = document.createDocumentFragment();
        let currentPosition = fragment;
        
        words.forEach(word => {
            const shouldWrap = wrappedWords < targetCount && 
                             word.trim().length > 0;
            
            if (shouldWrap) {
                const wrapper = document.createElement('span');
                wrapper.className = 'special-text';
                wrapper.textContent = word.replace(/\s+$/, '');
                currentPosition.appendChild(wrapper);
                wrappedWords++;
                
                // Preserve trailing whitespace
                const space = document.createTextNode(word.match(/\s+$/) || ' ');
                currentPosition.appendChild(space);
            } else {
                currentPosition.appendChild(
                    document.createTextNode(word)
                );
            }
        }); 

        // Replace original text node with processed content
        originalParent.replaceChild(fragment, node);
    });
});
</script>

